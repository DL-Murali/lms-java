import lombok.NoArgsConstructor;
import lombok.Setter;

@Getter
@Setter
@NoArgsConstructor
@AllArgsConstructor
public class NameFound extends RuntimeException {

	private static final long serialVersionUID = 1L;

	private String error;
	private String message;

}
